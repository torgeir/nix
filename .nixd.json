{
    "nixpkgs": {
        "expr": "import (builtins.getFlake (toString ./.)).inputs.nixpkgs { }"
    },
    "formatting": {
        "command": ["nixpkgs-fmt"]
    },
    "options": {
        "nixos": {
            "expr": "(builtins.getFlake (toString ./.)).nixosConfigurations.torgnix.options"
        },
        "home-manager": {
            "expr": "(builtins.getFlake (toString ./.)).nixosConfigurations.torgnix.config.home-manager.users.torgeir.options"
        }
    },
    "diagnostic": {
        "suppress": ["sema-escaping-with"]
    }
}
